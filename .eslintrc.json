{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "plugins": ["jest"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest/globals": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "no-underscore-dangle": "off",
    "quotes": ["error", "double"],
    "arrow-parens": ["error", "always"],
    "prefer-arrow-callback": "off",
    "no-plusplus": "off",
    "no-restricted-syntax": "off"
  }
}
